<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.ac.unsyiah.elektro.mobile.kalkulator15.MainActivityFragment"
    tools:showIn="@layout/activity_main">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/num1"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/num2"
        android:layout_toStartOf="@+id/num2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/num2"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_above="@+id/num4"
        android:layout_alignLeft="@+id/num5"
        android:layout_alignStart="@+id/num5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/num3"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/num2"
        android:layout_toEndOf="@+id/num2"
        android:width="100dp"
        android:onClick="pasKlik" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/num4"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_below="@+id/num1"
        android:layout_toLeftOf="@+id/num5"
        android:layout_toStartOf="@+id/num5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/num5"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_below="@+id/num2"
        android:layout_alignLeft="@+id/num8"
        android:layout_alignStart="@+id/num8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/num6"
        android:layout_alignTop="@+id/num5"
        android:layout_toRightOf="@+id/num5"
        android:layout_toEndOf="@+id/num5"
        android:width="100dp"
        android:onClick="pasKlik" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/num7"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_below="@+id/num4"
        android:layout_toLeftOf="@+id/num5"
        android:layout_toStartOf="@+id/num5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/num8"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_below="@+id/num5"
        android:layout_alignLeft="@+id/num0"
        android:layout_alignStart="@+id/num0" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/num9"
        android:layout_below="@+id/num6"
        android:layout_toRightOf="@+id/num8"
        android:layout_toEndOf="@+id/num8"
        android:width="100dp"
        android:onClick="pasKlik" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/num0"
        android:width="100dp"
        android:onClick="pasKlik"
        android:layout_below="@+id/num8"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
